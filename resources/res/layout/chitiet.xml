            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/linear11" android:paddingLeft="8dp" android:paddingTop="2dp" android:paddingRight="8dp" android:paddingBottom="4dp" android:layout_width="match_parent" android:layout_height="wrap_content">
